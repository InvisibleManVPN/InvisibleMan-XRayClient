<ResourceDictionary xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Color.Background.Normal" Color="#1e1e1e"/>
    <SolidColorBrush x:Key="Color.Background.Light" Color="#252526"/>

    <SolidColorBrush x:Key="Color.Bar" Color="#2d2d2d"/>

    <SolidColorBrush x:Key="Color.Logo.Normal" Color="#6dcc8e"/>
    <SolidColorBrush x:Key="Color.Logo.Button" Color="#252526"/>
    <SolidColorBrush x:Key="Color.Logo.Link" Color="#19b3e4"/>
    
    <SolidColorBrush x:Key="Color.Button.Confirm.Normal" Color="#6dcc8e"/>
    <SolidColorBrush x:Key="Color.Button.Confirm.Highlight" Color="#74da98"/>
    <SolidColorBrush x:Key="Color.Button.Confirm.Press" Color="#63bb82"/>

    <SolidColorBrush x:Key="Color.Button.Cancel.Normal" Color="#d95f5f"/>
